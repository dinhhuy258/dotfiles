cmd + ctrl - r :open -a kitty --args -e zsh -ci "run_cmds_fzf ~/.local/syscmds"
cmd + ctrl - a :open -a kitty --args -e zsh -ci "open_app_fzf"
cmd + ctrl - q :open -a kitty --args -e zsh -ci "kill_app_fzf"
cmd + ctrl - f :open -a kitty --args -e zsh -ci "lf"
cmd + ctrl - p :open -a kitty --args -e zsh -ci "pass_fzf"
