cmd + ctrl - r :open -a AlacrittyPopup --args --config-file=$HOME/.config/alacritty-popup.yml -e zsh -ci "run_cmds_fzf ~/.local/syscmds"
cmd + ctrl - a :open -a AlacrittyPopup --args --config-file=$HOME/.config/alacritty-popup.yml -e zsh -ci "open_app_fzf"
cmd + ctrl - q :open -a AlacrittyPopup --args --config-file=$HOME/.config/alacritty-popup.yml -e zsh -ci "kill_app_fzf"
cmd + ctrl - f :open -a AlacrittyPopup --args --config-file=$HOME/.config/alacritty-popup.yml -e zsh -ci "ranger"
