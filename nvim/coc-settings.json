{
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.messageLevel": "error",

  "diagnostic.enable": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.signOffset": 9999999,
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.refreshAfterSave": true,
  "diagnostic.displayByAle": true,
  "diagnostic.level": "error",
  "diagnostic.virtualText": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageTarget": "float",
  "diagnostic.highlightOffset": 1000,

  "suggest.triggerAfterInsertEnter": false,
  "suggest.minTriggerInputLength": 2,
  "suggest.timeout": 5000,
  "suggest.labelMaxLength": 400,
  "suggest.enablePreselect": false,
  "suggest.maxPreviewWidth": 160,
  "suggest.enablePreview": true,
  "suggest.floatEnable": true,
  "suggest.detailMaxLength": 150,
  "suggest.languageSourcePriority": 99,
  "suggest.numberSelect": false,
  "suggest.disableKind": false,
  "suggest.disableMenu": false,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.localityBonus": true,
  "suggest.acceptSuggestionOnCommitCharacter": false,
  "suggest.keepCompleteopt": false,
  "suggest.lowPrioritySourceLimit": 60,
  "suggest.highPrioritySourceLimit": 90,
  "suggest.noselect": true,
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.detailField":"abbr",
  "suggest.snippetIndicator": ":star2:",
  "suggest.triggerCompletionWait": 300,
  "suggest.echodocSupport": true,
  "suggest.autoTrigger": "always",
  "suggest.maxCompleteItemCount": 15,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },

  "yaml.schemas": {
    "kubernetes": "/*.yaml"
  },

  "python.jediEnabled": false,
  "python.linting.pylintEnabled": true,

  "java.configuration.runtimes": [
    {
      "name": "JavaSE-1.8",
      "path": "/Library/Java/JavaVirtualMachines/jdk1.8.0.jdk/Contents/Home",
      "default": true
    }
  ],

  "languageserver": {
    "clangd": {
      "command": "clangd",
      "rootPatterns": [
          "compile_flags.txt",
          "compile_commands.json",
          ".vim/",
          ".git/",
          ".hg/"
      ],
      "filetypes": [
          "c",
          "cpp",
          "objc",
          "objcpp"
      ]
    }
  }
}

