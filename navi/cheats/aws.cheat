% aws

# Download S3 object
aws s3api get-object --bucket <bucket> --key <key> <file>

# Put S3 object
aws s3api put-object --bucket <bucket> --key <key> --body <local_file>

# List S3 buckets
aws s3api list-buckets

$ bucket: aws s3api list-buckets --query "Buckets[].Name" --output text | tr '\t' '\n'
$ local_file: ls -1
